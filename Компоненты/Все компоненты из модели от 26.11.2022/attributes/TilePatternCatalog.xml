	<TilePattern Name="Herringbone">
		<Parameter Name="W" DefaultValue="180">
			<Label> _Tile_Width </Label>
		</Parameter>
		<Parameter Name="H" DefaultValue="80">
			<Label> _Tile_Height </Label>
		</Parameter>
		<Parameter Name="T" DefaultValue="100">
			<Label> _Tile_Thickness </Label>
		</Parameter>
		<Parameter Name="MH" DefaultValue="20">
			<Label> _Mortar_Height </Label>
		</Parameter>
		<Parameter Name="MW" DefaultValue="20">
			<Label> _Mortar_Width </Label>
		</Parameter>
		<HOffset>
			<Vector2D X="W+2*H+3*MW" />
		</HOffset>
		<VOffset>
			<Vector2D Y="W+2*H+3*MW" />
		</VOffset>
		<Color Color="50 60 70" />
		<!-- Tile 1 -->
		<Tile Angle="0" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="0" Y="0" />
			</TileOrigin>
			<Color Color="204 179 153" />
		</Tile>
		<!-- Tile 2 -->
		<Tile Angle="90" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="W" Y="H+MH" />
			</TileOrigin>
			<Color Color="204 179 153" />
		</Tile>
		<!-- Tile 3 -->
		<Tile Angle="90" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="W+H+MW" Y="0" />
			</TileOrigin>
			<Color Color="179 153 128" />
		</Tile>
		<!-- Tile 4 -->
		<Tile Angle="0" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="W+MW" Y="W+MH" />
			</TileOrigin>
			<Color Color="179 153 128" />
		</Tile>
		<!-- Tile 5 -->
		<Tile Angle="0" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="W+H+2*MW" Y="H+MH" />
			</TileOrigin>
			<Color Color="153 128 102" />
		</Tile>
		<!-- Tile 6 -->
		<Tile Angle="90" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="H" Y="2*(H+MH)" />
			</TileOrigin>
			<Color Color="153 128 102" />
		</Tile>
		<!-- Tile 7 -->
		<Tile Angle="90" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="W+2*(H+MW)" Y="W+H+2*MH" />
			</TileOrigin>
			<Color Color="128 102 77" />
		</Tile>
		<!-- Tile 8 -->
		<Tile Angle="0" Width="W" Height="H" Thickness="T">
			<TileOrigin>
				<Vector2D X="H+MW" Y="H+W+2*MH" />
			</TileOrigin>
			<Color Color="128 102 77" />
		</Tile>
