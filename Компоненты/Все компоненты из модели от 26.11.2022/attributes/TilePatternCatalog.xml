	<TilePattern Name="Checkered_Plate" LinearUnit="INCH">
		<Parameter Name="L" UnitType="Length" DefaultValue="1">
			<Label> _Pattern_Length </Label>
		</Parameter>
		<Parameter Name="W" UnitType="Length" DefaultValue=".125">
			<Label> _Pattern_Width </Label>
		</Parameter>
		<Parameter Name="T" UnitType="Length" DefaultValue=".5">
			<Label> _Plate_Thickness </Label>
		</Parameter>
		<Parameter Name="PS" UnitType="Length" DefaultValue=".25">
			<Label> _Pattern_Spacing </Label>
		</Parameter>
		<Parameter Name="BC" UnitType="Color" DefaultValue="200 200 200">
			<Label> _Background_Color </Label>
		</Parameter>
		<Parameter Name="PC" UnitType="Color" DefaultValue="100 100 100">
			<Label> _Pattern_Color</Label>
		</Parameter>
		<HOffset>
			<Vector2D X="L+2*PS+W" />
		</HOffset>
		<VOffset>
			<Vector2D Y="L+2*PS+W" />
		</VOffset>
		<Color Color="BC" />
		<!-- Tile 1 -->
		<Tile Angle="0" Width="L" Height="W" Thickness="T">
			<TileOrigin>
				<Vector2D X="0" Y="(L-W)*0.5" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
		<!-- Tile 2 -->
		<Tile Angle="0" Width="W" Height="L" Thickness="T">
			<TileOrigin>
				<Vector2D X="L+PS" Y="0" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
		<!-- Tile 3 -->
		<Tile Angle="0" Width="W" Height="L" Thickness="T">
			<TileOrigin>
				<Vector2D X="(L-W)*0.5" Y="(L+W)*0.5+PS" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
		<!-- Tile 4 -->
		<Tile Angle="0" Width="L" Height="W" Thickness="T">
			<TileOrigin>
				<Vector2D X="(L+W)*0.5+PS" Y="L+PS" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
	</TilePattern>
	<TilePattern Name="Expanded_Metal" AngleUnit="RADIAN">
		<Parameter Name="PW" UnitType="Length" DefaultValue="34">
			<Label> _Pattern_Width </Label>
		</Parameter>
		<Parameter Name="PL" UnitType="Length" DefaultValue="135.4">
			<Label> _Pattern_Length </Label>
		</Parameter>
		<Parameter Name="WT" UnitType="Length" DefaultValue="7.0">
			<Label> _Wire_Thickness </Label>
		</Parameter>
		<Parameter Name="PT" UnitType="Length" DefaultValue="4.5">
			<Label> _Panel_Thickness </Label>
		</Parameter>
		<Parameter Name="PC" UnitType="Color" DefaultValue="200 200 200">
			<Label> _Pattern_Color </Label>
		</Parameter>
		<Parameter Name="BC" UnitType="Color" DefaultValue="100 100 100">
			<Label> _Background_Color </Label>
		</Parameter>
		<HOffset>
			<Vector2D X="PL" />
		</HOffset>
		<VOffset>
			<Vector2D Y="PW" />
		</VOffset>
		<Color Color="BC" />
		<!-- Tile 1 -->
		<Tile Angle="atan(PW/PL)" Width="2*PL/cos(atan(PW/PL))" Height="WT" Thickness="PT">
			<TileOrigin>
				<Vector2D X="0-PL*0.5" Y="0-PW*0.5" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
		<!-- Tile 2 -->
		<Tile Angle="0-atan(PW/PL)" Width="2*PL/cos(atan(PW/PL))" Height="WT" Thickness="PT">
			<TileOrigin>
				<Vector2D X="0-PL*0.5" Y="PW*0.5" />
			</TileOrigin>
			<Color Color="PC" />
		</Tile>
	</TilePattern>
	<TilePattern Name="Grating" LinearUnit="INCH">
		<Parameter Name="CB" UnitType="Length" DefaultValue="4">
			<Label> _Cross_Bar_Centers </Label>
		</Parameter>
		<Parameter Name="BB" UnitType="Length" DefaultValue="1.1875">
