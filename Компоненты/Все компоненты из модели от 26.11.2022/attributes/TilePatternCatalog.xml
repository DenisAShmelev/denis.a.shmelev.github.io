		<Parameter Name="BBW" UnitType="Length" DefaultValue="0.25">
			<Label> _Bearing_Bar_Width </Label>
		</Parameter>
		<Parameter Name="CBW" UnitType="Length" DefaultValue="0.375">
			<Label> _Cross_Bar_Width </Label>
		</Parameter>
		<Parameter Name="BC" UnitType="Color" DefaultValue="250 250 250">
			<Label> _Background_Color </Label>
		</Parameter>
		<Parameter Name="PC" UnitType="Color" DefaultValue="25 25 25">
			<Label> _Pattern_Color </Label>
		</Parameter>
		<HOffset>
			<Vector2D X="CB+CBW" Y="0" />
		</HOffset>
		<VOffset>
			<Vector2D X="0" Y="BB+BBW" />
		</VOffset>
		<Color Color="PC" />
		<Tile Angle="0" Width="CB" Height="BB" Thickness="GT">
			<TileOrigin>
				<Vector2D X="0" Y="0" />
			</TileOrigin>
			<Color Color="BC" />
		</Tile>
	</TilePattern>
	<TilePattern Name="Wire_Mesh" LinearUnit="INCH">
		<Parameter Name="CTC" UnitType="Length" DefaultValue="2">
			<Label> _Center_to_Center </Label>
		</Parameter>
		<Parameter Name="WT" UnitType="Length" DefaultValue=".1875">
			<Label> _Wire_Thickness </Label>
		</Parameter>
		<Parameter Name="PT" UnitType="Length" DefaultValue="1">
			<Label> _Panel_Thickness </Label>
		</Parameter>
		<Parameter Name="BC" UnitType="Color" DefaultValue="200 200 200">
			<Label> _Background_Color </Label>
		</Parameter>
		<Parameter Name="HC" UnitType="Color" DefaultValue="100 100 100">
			<Label> _HWire_Color </Label>
		</Parameter>
		<Parameter Name="VC" UnitType="Color" DefaultValue="90 110 130">
			<Label> _VWire_Color </Label>
		</Parameter>
		<HOffset>
			<Vector2D X="CTC+WT" />
		</HOffset>
		<VOffset>
			<Vector2D Y="CTC+WT" />
		</VOffset>
		<Color Color="BC" />
		<!-- Tile 1 -->
		<Tile Angle="0" Width="CTC" Height="WT" Thickness="PT">
			<TileOrigin>
				<Vector2D X="0" Y="-WT" />
			</TileOrigin>
			<Color Color="HC" />
		</Tile>
		<!-- Tile 2 -->
		<Tile Angle="0" Width="WT" Height="CTC" Thickness="PT">
			<TileOrigin>
				<Vector2D X="(CTC*0.5)-(WT*0.5)" Y="0" />
			</TileOrigin>
			<Color Color="VC" />
		</Tile>
		<!-- Tile 3 -->
		<Tile Angle="0" Width="WT" Height="CTC" Thickness="PT">
			<TileOrigin>
				<Vector2D X="CTC" Y="(CTC*0.5)+(WT*0.5)" />
			</TileOrigin>
			<Color Color="VC" />
		</Tile>
		<!-- Tile 4 -->
		<Tile Angle="0" Width="CTC" Height="WT" Thickness="PT">
			<TileOrigin>
				<Vector2D X="(CTC*0.5)+(WT*0.5)" Y="(CTC*0.5)-(WT*0.5)" />
			</TileOrigin>
			<Color Color="HC" />
		</Tile>
	</TilePattern>
</TilePatternCatalog>
